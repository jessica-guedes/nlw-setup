{
  "expo": {
   "name": "Habits: Jessica",
   "slug": "HabitsJessica",
   "version": "1.0.0",
   "platforms": [
     "ios",
     "android"
   ],
   "privacy": "public",
   "orientation": "portrait",
   "icon": "./assets/icon.png",
   "userInterfaceStyle": "light",
   "splash": {
     "image": "./assets/splash.png",
     "resizeMode": "contain",
     "backgroundColor": "#09090A"
   },
   "updates": {
     "fallbackToCacheTimeout": 0
   },
