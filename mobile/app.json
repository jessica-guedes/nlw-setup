   },
   "android": {
     "adaptiveIcon": {
       "foregroundImage": "./assets/adaptive-icon.png",
       "backgroundColor": "##09090A"
     },
           "package": "jessica.nlwsetup"

   },
   "web": {
     "favicon": "./assets/favicon.png"
   }
 }
}